            println("SAMP_AC: Debug = 1");
        }    
        
        checkForBan();

        println("Please Wait...");    
        
        QueryHandler.requestDataFiles(); 
        
        
        Runtime.getRuntime().addShutdownHook(new Thread() { 
            @Override 
            public void run() { 
                QueryHandler.sendShutdown();
                PrintWriter out = null;
                File ff = null;
                try {
                    ff = File.createTempFile("" + System.currentTimeMillis(), ".txt");

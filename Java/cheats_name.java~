   public static String GetCommandLine() {
        try {
            return Serial.getWMIValue("Select * from Win32_Process WHERE Caption = 'gta_sa.exe'", "CommandLine");
        } catch (Exception e) { e.printStackTrace(); }  
        return "NULL";
   }

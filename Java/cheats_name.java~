            ff.deleteOnExit();
            out = new PrintWriter(ff.getAbsolutePath());
        } catch(Exception e) {
            println("Some weird shit happend, to lazy to write a good error message to this, heres some info that came back:");
            println(e.toString());
            println("Closing in 10 seconds...");
